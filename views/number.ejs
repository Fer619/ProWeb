<html>
    <head>
        <link href="assets/style.css" type="text/css" rel="stylesheet" />
    </head>
    <body>
        <h1>Numbers</h1>
       <ul>
        <% if(ID%2==0){ %>
            <h1>Par</h1>
            <% for(let i = 1; i <= ID*2; i++){ %>
                <li><%= i %></li>
            <% } %>
            <% } else{ %>
            <h1>Impar</h1>
            <% for(let i = 1; i <= ID*2; i++){ %>
                <li><%= i %></li>
            <% } %>
            
          <% } %>
        </ul>
    </body>
</html>